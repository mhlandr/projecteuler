#include<vector>  
#include<iostream>
using namespace std;
long long b=1, c;
int a;
string v,s;
int main(){
	int i, j, k;
	for (i = 1; i<=1000; i++) {
		for (j = i + 1; j <= 1000; j++) {
			for (k = j + 1; k <= 1000; k++) {
				if (i * i + j * j == k * k && i + j + k == 1000) {
					cout << i * j * k;
					return 0;
				}
			}
		}
	}

	return 0;
} 
